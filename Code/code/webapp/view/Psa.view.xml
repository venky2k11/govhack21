<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.saptraining.untitledPrototype.controller.Psa" xmlns="sap.m" xmlns:layout1="sap.ui.layout">
    <Page showHeader="true" title="Personalized Super Assessment" showFooter="false" showNavButton="true" navButtonPress="_onPageNavButtonPress">
        <content>
            <layout1:BlockLayout background="Default" visible="true">
                <layout1:content>
                    <layout1:BlockLayoutRow scrollable="false">
                        <layout1:content>
                            <layout1:BlockLayoutCell width="1" title="Total Super available" titleAlignment="Begin" titleLevel="Auto">
                                <layout1:content>
                                    <Text text="$ 117,170.92" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </layout1:content>
                            </layout1:BlockLayoutCell>
                            <layout1:BlockLayoutCell width="1" title="Expected Super" titleAlignment="Begin" titleLevel="Auto">
                                <layout1:content>
                                    <Text text="$ 130000" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </layout1:content>
                            </layout1:BlockLayoutCell>
                            <layout1:BlockLayoutCell width="2" title="Overall Health Status : Amber" titleAlignment="Center" titleLevel="Auto">
                                <layout1:content>
                                    <TextArea value="Living standards of individuals similar to yours were compared." rows="2" cols="50" maxLength="0" wrapping="Soft" enabled="true" required="false" valueStateText="Invalid entry" visible="true"/>
                                </layout1:content>
                            </layout1:BlockLayoutCell>
                        </layout1:content>
                    </layout1:BlockLayoutRow>
                </layout1:content>
            </layout1:BlockLayout>
            <Image src="/webapp/resources/chart1.png" height="281.98px" width="320px" visible="true" densityAware="false"/>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer width=""/>
                    <Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
